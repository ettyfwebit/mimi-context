# Required: Backend API base URL
VITE_MIMI_API_BASE=http://localhost:8080

# Optional: API key for authentication
# VITE_MIMI_API_KEY=your-api-key-here

# Optional: Default number of search results (default: 5)
# VITE_MIMI_DEFAULT_TOP_K=5

# Optional: Default model for chat agent
# VITE_MIMI_DEFAULT_MODEL=gpt-3.5-turbo

# Optional: Confidence threshold for low-confidence warnings (default: 0.30)
# VITE_MIMI_CONFIDENCE_THRESHOLD=0.30
