# Frontend Environment Variables Template
# Copy this file to .env.local and fill in the actual values
# NEVER commit the actual .env.local file to version control

# Application Configuration
VITE_APP_NAME=Mimi Web
VITE_APP_VERSION=1.0.0

# Backend API Configuration
VITE_MIMI_API_BASE=http://localhost:8081
VITE_MIMI_API_TIMEOUT=10000

# Authentication (if implementing auth)
# VITE_MIMI_API_KEY=your-api-key-here  # SECURITY: Never commit actual keys!

# Search Configuration
VITE_MIMI_DEFAULT_TOP_K=5
VITE_MIMI_CONFIDENCE_THRESHOLD=0.30

# Chat Agent Configuration
VITE_MIMI_DEFAULT_MODEL=gpt-3.5-turbo
VITE_MIMI_MAX_TOKENS=2000

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# External Services
# VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
# VITE_SENTRY_DSN=your_sentry_dsn_here

# Development
VITE_DEBUG_MODE=false
VITE_MOCK_API=false
